<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Start</th>
      <th>End</th>
      <th>Location</th>
      <th>Url1</th>
      <th>Url2</th>
      <th>Url3</th>
      <th>Url4</th>
      <th>Url5</th>
      <th>Remarks</th>
      <th>Allday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.start %></td>
        <td><%= event.end %></td>
        <td><%= event.location %></td>
        <td><%= event.url1 %></td>
        <td><%= event.url2 %></td>
        <td><%= event.url3 %></td>
        <td><%= event.url4 %></td>
        <td><%= event.url5 %></td>
        <td><%= event.remarks %></td>
        <td><%= event.allDay %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
