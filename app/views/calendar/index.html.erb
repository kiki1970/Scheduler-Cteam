        <label for="reamrks">備考</label></br>
        <input type="text" name="remarks" id="remarks" value=""/></br>
        <label for="allDay">終日</label></br>
        <input type="checkbox" name="allDay" id="allDay" value="1"/></br>
	</form>
</div>
